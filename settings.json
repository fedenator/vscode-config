{
    "editor.insertSpaces": false,
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "editor.cursorStyle": "block-outline",
    "editor.minimap.enabled": false,
    "workbench.activityBar.visible": false,
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.zoomLevel": -1,
    "editor.accessibilityPageSize": 13,
    "editor.fontSize": 12,
    "java.configuration.checkProjectSettingsExclusions": false,
    "debug.showInlineBreakpointCandidates": false,
    "java.semanticHighlighting.enabled": true,
    "java.requirements.JDK11Warning": false,
    "workbench.editor.scrollToSwitchTabs": true,
    "editor.rename.enablePreview": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "search.useReplacePreview": false,
    "go.testEnvFile": "",
    "go.formatTool": "goimports",
    "debug.internalConsoleOptions": "neverOpen",
    "workbench.startupEditor": "newUntitledFile",
    "rust-analyzer.lens.enable": false,
    "rust-analyzer.procMacro.enable": true,
    "vsintellicode.sql.completionsEnabled": false,
    "errorLens.errorColor": "rgba(250, 102, 95, 0.1)",
    "errorLens.warningColor": "rgba(252, 253, 100, 0.1)",
    "todohighlight.keywordsPattern": "(TODO|FIXME|HACK|DEBUG)\\((.*)\\):",
    "todohighlight.defaultStyle": {
        "color": "#00ff00",
        "backgroundColor": "#494949",
        "overviewRulerColor": "#00ab00",
        "cursor": "pointer",
        "border": "1px solid #eee",
        "borderRadius": "2px",
        "isWholeLine": true,
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "search.searchOnType": false,
    "java.project.importOnFirstTimeStartup": "automatic",
    "editor.cursorBlinking": "smooth"
}